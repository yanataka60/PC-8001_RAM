/* INPUTS... */
Pin   1 = IOREQ;
Pin   2 = MREQ;
Pin   3 = A7;
Pin   4 = A6;
Pin   5 = A5;
Pin   6 = A4;
Pin   7 = A3;
Pin   8 = A2;
Pin   9 = A1;
Pin  10 = A0;
Pin  11 = D4;
Pin  13 = D0;
Pin  20 = A15;
