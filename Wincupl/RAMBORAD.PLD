/* OUTPUTS... */
Pin  15 = CS;
Pin  16 = WE;
Pin  17 = LATCHRD;
Pin  18 = LATCHWR;
Pin  19 = ROMDS;

/** Logic **/
/* $E2 */
E2 = !IOREQ & !WR & A7 & A6 & A5 & !A4 & !A3 & !A2 & A1 & !A0;
RAMRD = E2 & D0;
RAMWR = E2 & D4;
ROMRD = E2 & !D0;
ROMWR = E2 & !D4;
LATCHRD = (LATCHRD # RAMRD) & !(ROMRD # !RESET);
LATCHWR = (LATCHWR # RAMWR) & !(ROMWR # !RESET);
CS = !(((LATCHRD & !RD) # (LATCHWR & !WR)) & !A15 & !MREQ);
WE = !(LATCHWR & !WR & !MREQ & !A15);
ROMDS = !LATCHRD;

